  <div>
    <p>検索したキーワード : <%= @keyword %></p>
    <p>Eventon: <%= @eventonnumber %></p>
    <p>Connmapss: <%= @connpassnumber  %></p>
    <p>Atnd: <%= @atndnumber %></p>
    <p>Doorkeeper: <%= @doorkeepernumber %></p>

    <% @events.each do |event| %>
      <div class="row event-card">
        <div class="col s12 m3">

        </div>

        <div class="col s12 m9 l8">
          <div class="card white-gray darken-1 card-panel hoverable">
            <div class="card-content grey-text">
              <span class="card-title">
                <%= event['title'] %>
              </span>
              <p><%= event['started_at'] %></p>
            </div>
            <div class="card-action">
              <%= link_to "ページに移動する", "#{event['event_url']}" %>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
  <!-- エラーメッセージを表示-->
message: <%= @message%>
